{
  "taskDefinition": {
    "family": "hello-world",
    "containerDefinitions": [
      {
        "name": "hello-world",
        "image": "132060109857.dkr.ecr.eu-west-2.amazonaws.com/hello-world:latest",
        "cpu": 100,
        "portMappings": [
          {
            "protocol": "tcp",
            "containerPort": 8080,
            "hostPort": 80
          }
        ],
        "memory": 128,
        "essential": true
      }
    ]
  }
}